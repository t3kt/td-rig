{
  "workspace": {
    "Name": "rig",
    "Exposesubcomps": true,
    "Subcomptags": "tmod vjzmod4",
    "Folderlayout": "subdirs"
  },
  "recorder": {
    "Folder": "S:/output/rig",
    "Basename": {
      "$": "ipar.workspace.Name"
    },
    "Includedate": true,
    "Datetype": "today",
    "Customdate": "",
    "Suffix": {
      "$": "ipar.editorState.Name"
    },
    "Addresolutionsuffix": true,
    "Addvcodecsuffix": true,
    "Addacodecsuffix": true,
    "Addfpssuffix": true,
    "Outputresolution": "useinput",
    "Resolution1": 3840,
    "Resolution2": 2160,
    "Videocodec": "hap",
    "Videotype": "movie",
    "Audiocodec": "alac",
    "Imagefiletype": "tiff",
    "Audiobitrate": "b128",
    "Quality": 1.35,
    "Fps": 30,
    "Audioenabled": false,
    "Moviepixelformat": "rgb",
    "Bitratemode": "variable",
    "Entropymode": "autoselect",
    "Hapsecondarycompression": true,
    "Profile": "autoselect",
    "Avgbitrate": 8000,
    "Peakbitrate": 10000,
    "Keyframeinterval": 15
  },
  "libraryLoader": {
    "Libraries": {
      "$": "None"
    }
  },
  "editorUIState": {
    "Selectedview": "preview_panel",
    "Selectedleftpanel": "component_picker",
    "Selectedrightpanel": "none",
    "Previewshowaudio": false,
    "Previewshowvideo": true,
    "Previewshowvideoinfo": false
  },
  "editorViews": {
    "Customviews": [
      {
        "name": "testCustom",
        "label": "Test Custom View",
        "copyOf": {"$":  "parent.editorRoot.op('TEST_customView')"}
      }
    ]
  }
}